---
title: Posts
---

{% import "partials/post.njk" as post with context %}
<section class="h-feed">
    <h1 class="p-name">{{ title }}</h1>
    <ul>
        {% for page in (posts | reverse) %}
        <li>
            {{ post.postStart(page) }}
            {{ post.post(page) }}
            {{ post.postEnd(page) }}
        </li>
        {% endfor %}
    </ul>
</section>
