{
	"name": "karawale.in",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"postinstall": "mkdir -p static/fonts && cp node_modules/@fontsource-variable/eb-garamond/files/eb-garamond-latin-wght-normal.woff2 static/fonts/ && cp node_modules/@fontsource-variable/eb-garamond/files/eb-garamond-latin-wght-italic.woff2 static/fonts/ && cp node_modules/@fontsource-variable/victor-mono/files/victor-mono-latin-wght-normal.woff2 static/fonts/",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/enhanced-img": "^0.9.3",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"mdsvex": "^0.12.6",
		"sass-embedded": "^1.97.2",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@fontsource-variable/eb-garamond": "^5.2.7",
		"@fontsource-variable/victor-mono": "^5.2.8",
		"@lucide/svelte": "^0.562.0",
		"utopia-core-scss": "^1.3.1"
	}
}
